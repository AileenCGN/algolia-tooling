  },
  "eslintConfig": {
    "extends": [
      "airbnb-base",
      "plugin:ghost/browser"
    ],
    "plugins": [
      "ghost"
    ]
  },
  "devDependencies": {
    "dotenv-webpack": "1.7.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.1.0",
    "eslint-plugin-ghost": "1.3.0",
    "eslint-plugin-import": "2.20.2",
    "mocha": "7.1.1",
    "should": "13.2.3",
    "sinon": "9.0.2"
  },
  "dependencies": {
    "netlify-lambda": "1.6.3"
  }
}
